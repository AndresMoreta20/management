@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center" >
    <h1 class="display-4">Welcome</h1>
    <p>Application for employee list management.</p>
    <div id="app">
        <h1 class="display-4">{{message}}</h1>
    </div>
